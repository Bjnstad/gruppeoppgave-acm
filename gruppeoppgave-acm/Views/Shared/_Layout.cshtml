<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>ACM Film</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
</head>
<body>

    <div class="container">
        <div class="row">
            <div class="col col-lg-offset-1 col-lg-2">
                <h1>
                    <a href="/">
                        ACM
                    </a>
                </h1>
            </div>
            <div class="col col-md-offset-3 col-md-6>
                <nav>
                    <a href="#">Link 1</a> |
                    <a href="#">Link 2</a> |
                    <a href="#">Link 3</a> |
                    <a> @if (Session["id"] == null)
                    {
                        <a>@Html.ActionLink("Login", "../Login/Login") </a>
                    }
                    else { <a href="/Login/Logout">Log out</a> }



                        </a>
                </nav>
            </div>
        </div>



        <div class="body-content">
            @RenderBody()

            <footer>
                <p>&copy; @DateTime.Now.Year - ACM</p>
            </footer>
        </div>
    </div>

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/jquery"></script>



    <script>
        $(document).ready(function () {
            $(".link--category").click(function (obj) {
