@model gruppeoppgave_acm.Models.Movie


<div class="col col-md-4 col-xs-6">
    <div class="movie-thumbnail" style="background-image: url('@Model.Thumbnail')">
        <h3>
            @Model.Title
        </h3>
        <br />
        <p>
            @Model.Price
        </p>
        <br />
        <p>
            @Model.Description
        </p>
    </div>
</div>
